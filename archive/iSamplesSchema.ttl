isam:Action
  rdf:type owl:Class ;
  rdfs:comment "an action at a given date, to track history of sample and its metadata"@en ;
  rdfs:label "Action"@en ;
  rdfs:subClassOf owl:Thing ;
  skos:prefLabel "Action"@en ;
